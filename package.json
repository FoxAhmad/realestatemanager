{
  "name": "uh-crm",
  "version": "1.0.0",
  "description": "Developer-side CRM system for deal management",
  "main": "server/index.js",
  "scripts": {
    "start": "node server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "client": "cd client && npm start",
    "install-all": "npm install && cd client && npm install",
    "setup": "node setup.js",
    "migrate": "node migrate-to-dealers.js",
    "migrate-customers": "node migrate-customers-dealer.js",
    "migrate-inventory": "node migrate-inventory-deals.js",
    "migrate-plots": "node migrate-inventory-to-plots.js"
  },
  "keywords": [
    "crm",
    "mern",
    "postgresql"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.16.3",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "nodemon": "^3.0.2"
  }
}